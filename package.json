{
  "name": "nuxt-driver.js",
  "version": "0.0.1",
  "description": "A simple wrapper around the driver.js package(all credits to them) for creating guided tours and feature introductions for your Nuxt.js applications.",
  "repository": "your-org/nuxt-driver.js",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Behon Baker",
    "email": "behon.baker@yahoo.com",
    "url": "https://behonbaker.com/"
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "driver.js",
    "guided-tour",
    "feature-introduction"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "afterClone": "npm i && cd ./playground && npm i && cd .. && npm run dev:prepare",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.11.2",
    "driver.js": "^1.3.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.2.0",
    "@nuxt/eslint-config": "^0.3.12",
    "@nuxt/module-builder": "^0.6.0",
    "@nuxt/schema": "^3.11.2",
    "@nuxt/test-utils": "^3.12.1",
    "@types/node": "^20.12.11",
    "changelogen": "^0.5.5",
    "eslint": "^9.2.0",
    "nuxt": "^3.11.2",
    "typescript": "5.4.5",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.17"
  }
}
